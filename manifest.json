{
  "manifest_version": 2,
  "name": "Space Tabs",
  "version": "1.0.1",
  
  "description": "Space images in the New Tab",

  "icons": {
    "128": "icon.png"
  },
  
  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],

  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
