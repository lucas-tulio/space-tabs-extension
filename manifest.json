{
  "manifest_version": 2,
  "name": "Space Tabs",
  "version": "1.0",
  
  "description": "Space images in New Tab. Powered by Astrobin.com",
  
  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],

  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
